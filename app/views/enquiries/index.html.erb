<div class="container">
  <div class="container-fluid">
  <legend>Listing all Enquiries</legend>
  <div class="row">
    <div class="col-lg-12">
      <div class="well bs-component">
          <table class="table table-striped table-bordered dt-responsive nowrap myTable">
            <thead>
              <tr>
                <th>Name</th>
                <th>Mobile</th>
                <th>Enquired at</th>
              </tr>
            </thead>

            <tbody>
              <% @enquiries.each do |enquire| %>
                <tr>
                  <td><%= enquire.name %></td>
                  <td><%= enquire.mobile %></td>
                  <td><%= enquire.created_at.strftime("%b %e, %l:%M %p") %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>